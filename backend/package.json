{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express API",
  "main": "src/index.ts",
  "dependencies": {
    "@prisma/client": "^2.2.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/uuid": "^8.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "env-var": "^6.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "typescript": "^3.9.6",
